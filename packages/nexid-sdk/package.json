{
  "name": "@nexshop/nexid-sdk",
  "version": "0.1.0",
  "type": "module",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup ./src/index.ts --dts --format esm,cjs,iife --global-name NexID --minify --sourcemap --clean",
    "dev": "tsup ./src/index.ts --dts --format esm,cjs,iife --global-name NexID --watch --sourcemap"
  },
  "devDependencies": {
    "tsup": "8.3.0",
    "typescript": "5.8.3"
  }
}
